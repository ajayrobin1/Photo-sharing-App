{
  "name": "artgallery",
  "version": "0.1.0",
  "homepage": "/",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.3",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-inner-image-zoom": "^3.0.2",
    "react-lazy-load-image-component": "^1.6.0",
